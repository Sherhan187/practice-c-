#include <iostream>
#include <cmath>
using namespace std;
int main() {
double a, b;
cin >> a;
b = (a * 1.8) + 32;
cout << b;
}

23**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
	double x, a, y, k, y2, k2;
	cin >> x >> a >> y >> k;
	y2 = (y * a) / x;
	k2 = (k * x) / a;
	cout << y2 << " "<<k2;
}

24**

#include <iostream>
using namespace std; 
int main(){
int days,sale;
float sum = 0.0f;
cin >> days >> sale >> sum;
sale /= 100;
for (int i = 0; i < days; i++)
{
sum += 3; 
sum += sum * sale; 
}
cout << sum << endl;
}

