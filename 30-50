#include <iostream>
#include  <cmath>
using namespace std;

int main()
{
    int a;
    cin>>a;
    if (a > -10 && a < 10){
        cout<<a+5<<endl;
    }
  else  {
      cout<<a-10<<endl;
  }
  return 0;
}

