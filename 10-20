
11**
#include <iostream>
#include <cmath>
using namespace std;
int main() {
    float x;
    cin >> x;
    cout << (abs(x - 5) - sin(x)) / 3 + sqrt(x * x + 2014) * cos(2 * x) - 3 << endl;
    return 0;
}

12**

