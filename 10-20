#include <cmath>

using namespace std;
int main() {
    float x;
  cin>>x;;
  cout<< (exp(x-2)) + (abs(sin(x)))- (pow(x,4)) * (cos(1/x))<<endl;
  return 0;
    }


13**
#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
double a = 0.1;
double b = 0.2;
double x = 1;
double s = pow((pow(x, 2) + b), 1.0 / 5) - ((pow(b, 2) * pow(sin(x + a), 3)) / x);
cout << s;
return 0;
}
14**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
int a, b;
cin >> a >> b;
int d = a + b;
int e = a * b;
cout <<"sum="<< d << endl <<"Proizv="<< e;
}
15**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
int a;
cin >> a;
int b = pow(a, 2);
int c = pow(a, 3);
cout << b << endl << c;
return 0;
}

16**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
double a, b, c;
cin >> a >> b >> c;
double d = a * 2;
double v = b / 3;
double e = pow(c,2);
double m = d + v + e;
cout << m; 
return 0;
}
17**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
double a, b, c;
cin >> a >> b >> c;
double d = (a + b + c) / 3;
double s = (a * 2) - (b * 2) - (c * 3);
cout << "Arifmetic=" << d <<endl<<"Raznost=" << s;
return 0;
}

18**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
int a;
cin >> a;
int b = a * 4;
int c = pow(a, 2);
cout << "Perimetr=" << b<< endl<< "Area=" << c;
return 0;
}
19**
#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
int a, b;
cin >> a>>b;
int b = a * 4;
int c = pow(a, 2);
cout << "Perimetr=" << b << endl << "Area=" << c;
return 0;
}

20**

#include <iostream>
#include <cmath>
#include <cstring>
#include <cctype>
#include<string>
using namespace std;
int main() {
int a, b;
cin >> a >> b;
cout << "a)" << 0.3 * a << " " << 0.4 * b<<endl;
cout << "b)" << 3 * (1.8 * a) << " " << 3 * 2 * b;
return 0;
}
